<div class="post_container">
    <div class="new_post">
        <h1>
            Edit a picture
        </h1>
        <%= simple_form_for @job do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :contact_email %>
        <%= f.input :is_hidden %>
        <%= f.input :picture %>
        <%= f.submit "Submit",class:"btn btn-default" %>
        <% end %>
    </div>
</div>
